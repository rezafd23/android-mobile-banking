    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@color/appBackground</item>
        <item name="toolbarStyle">@style/e5tToolbarStyle</item>
        <item name="actionBarStyle">@style/e5tActionBarStyle</item>
        <item name="colorControlNormal">@color/titleTextColor</item>
        <item name="homeAsUpIndicator">@drawable/ic_toolbar_back_dark</item>
        <item name="android:windowAnimationStyle">@style/WindowAnimationTransition</item>
        <item name="android:statusBarColor">@color/statusBarColor</item>
        <item name="android:windowLightStatusBar" ns1:targetApi="m">true</item>
    </style>
    <style name="AppTheme.DarkActionBar">
        <item name="actionBarStyle">@style/e5tActionBarStyle.DarkActionBar</item>
        <item name="colorControlNormal">@android:color/white</item>
        <item name="homeAsUpIndicator">@drawable/ic_toolbar_back_tint</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_in_bottom</item>
        <item name="android:windowExitAnimation">@anim/slide_out_bottom</item>
    </style>
    <style name="DialogAnimationFade">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>
    <style name="DialogNoAnimation">
        <item name="android:windowEnterAnimation">@null</item>
        <item name="android:windowExitAnimation">@null</item>
    </style>
    <style name="ElementButton" parent="@style/Widget.AppCompat.Button.Borderless">
        <item name="android:background">@drawable/element_round_button</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:paddingStart">20dp</item>
        <item name="android:paddingEnd">20dp</item>
        <item name="android:paddingTop">14dp</item>
        <item name="android:paddingBottom">14dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">center</item>
        <item name="android:elevation">0dp</item>
        <!--<item name="android:stateListAnimator">@null</item>-->
    </style>
    <style name="ElementTextButton" parent="@style/ElementButton">
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:background">?android:attr/selectableItemBackgroundBorderless</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">17sp</item>
    </style>
    <style name="FullScreenDialogStyle" parent="Theme.AppCompat.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="android:textColor">@color/textColor</item>

        <item name="android:statusBarColor">@color/statusBarColor</item>

        <!-- Set this to true if you want Full Screen without status bar -->
        <item name="android:windowFullscreen">false</item>

        <item name="android:windowIsFloating">false</item>

        <!-- This is important! Don't forget to set window background -->
        <item name="android:windowBackground">@android:color/white</item>
    </style>
    <style name="WindowAnimationTransition">
        <!--<item name="android:windowEnterAnimation">@anim/fade_in</item>-->
        <!--<item name="android:windowExitAnimation">@anim/fade_out</item>-->

        <item name="android:activityOpenEnterAnimation">@anim/slide_in_right</item>
        <item name="android:activityOpenExitAnimation">@anim/slide_out_left</item>

        <item name="android:activityCloseEnterAnimation">@anim/slide_in_left</item>
        <item name="android:activityCloseExitAnimation">@anim/slide_out_right</item>
    </style>
    <style name="e5tActionBarStyle" parent="Base.Widget.AppCompat.Light.ActionBar.Solid">
        <item name="titleTextStyle">@style/e5tToolbarTitleAppearance</item>
        <item name="subtitleTextStyle">@style/e5tToolbarSubtitleAppearance</item>
        <item name="background">@color/actionBarBg</item>
    </style>
    <style name="e5tActionBarStyle.DarkActionBar">
        <item name="titleTextStyle">@style/e5tToolbarTitleAppearance.DarkActionBar</item>
        <item name="subtitleTextStyle">@style/e5tToolbarSubtitleAppearance.DarkActionBar</item>
        <item name="background">@android:color/black</item>
    </style>
    <style name="e5tToolbarStyle" parent="Widget.AppCompat.Toolbar">
        <item name="titleTextAppearance">@style/e5tToolbarTitleAppearance</item>
        <item name="subtitleTextAppearance">@style/e5tToolbarSubtitleAppearance</item>
    </style>
    <style name="e5tToolbarSubtitleAppearance" parent="TextAppearance.Widget.AppCompat.Toolbar.Subtitle">
        <item name="android:textColor">@color/titleTextColor</item>
        <item name="android:textColorSecondary">@color/titleTextColor</item>
    </style>
    <style name="e5tToolbarSubtitleAppearance.DarkActionBar">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textColorSecondary">@android:color/white</item>
    </style>
    <style name="e5tToolbarTitleAppearance" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textColor">@color/titleTextColor</item>
        <item name="android:textColorSecondary">@color/titleTextColor</item>
    </style>
    <style name="e5tToolbarTitleAppearance.DarkActionBar">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textColorSecondary">@android:color/white</item>
    </style>
</resources>