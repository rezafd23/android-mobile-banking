<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relevan_panel"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nama Gadis Ibu Kandung"
        android:textSize="@dimen/text_xs_14"
        android:textStyle="bold"
        android:fontFamily="@font/opensans_bold"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_nama_ibu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="Ade Kartini"
        android:textSize="@dimen/text_xs_12" />


    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/default_margin"
        android:text="Nama Lengkap Kerabat"
        android:textSize="@dimen/text_xs_14"
        android:textStyle="bold"
        android:fontFamily="@font/opensans_bold"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_nama_kerabat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="Azhar Suherman"
        android:textSize="@dimen/text_xs_12" />

    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/default_margin"
        android:text="Hubungan"
        android:textSize="@dimen/text_xs_14"
        android:textStyle="bold"
        android:fontFamily="@font/opensans_bold"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_hubungan_kerabat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="Kakak Kandung"
        android:textSize="@dimen/text_xs_12" />

    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/default_margin"
        android:text="No Telepon yang bisa Dihubungi"
        android:textSize="@dimen/text_xs_14"
        android:textStyle="bold"
        android:fontFamily="@font/opensans_bold"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_no_telp_kerabat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="081283883488"
        android:textSize="@dimen/text_xs_12" />

    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/default_margin"
        android:text="Alamat Kerabat"
        android:textSize="@dimen/text_xs_14"
        android:textStyle="bold"
        android:fontFamily="@font/opensans_bold"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_alamat_kerabat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="Jl Keselamatan No 57"
        android:textSize="@dimen/text_xs_12" />

</androidx.appcompat.widget.LinearLayoutCompat>